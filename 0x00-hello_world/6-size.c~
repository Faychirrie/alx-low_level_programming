#include <stdio.h>
#include <conio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
  int intType;
  float floatType;
  long long longType;
  char charType;
  printf("Size of a char:\n",sizeof(charType));
  printf("Size of an int:%d\n", sizeof(intType));
  printf("Size of a long long int:\n",sizeof(longType));
  printf("Size of a float:\n",sizeof(floatType));
  return (0);
}
